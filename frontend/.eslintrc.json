{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "no-underscore-dangle": "off",
      "class-methods-use-this": "off",
      "default-case": "off"
    },
    "globals": {
      "Chart": true,
      "Color": true
    }

}
